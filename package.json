{
  "name": "@angular-material-extensions/core",
  "description": "Set of components, directives and services to boost the app development with angular material 2",
  "version": "0.1.0",
  "homepage": "https://github.com/angular-material-extensions/core",
  "author": {
    "name": "Anthony Nahas",
    "url": "https://github.com/angular-material-extensions"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/angular-material-extensions/core.git"
  },
  "license": "MIT",
  "schematics": "./schematics/collection.json",
  "keywords": [
    "angular",
    "material2",
    "material design",
    "library",
    "core",
    "components",
    "directives",
    "utilities",
    "extension"
  ],
  "bugs": {
    "url": "https://github.com/angular-material-extensions/core/issues"
  },
  "private": true,
  "scripts": {
    "build": "gulp build",
    "build:doc": "gulp build:doc",
    "build:schematics": "gulp build:schematics",
    "build:watch:fast": "gulp build:watch-fast",
    "link": "gulp link",
    "unlink": "gulp unlink",
    "test": " gulp test",
    "demo": " gulp build:demo",
    "check-travis": " travis-status --repo=angular-material-extensions/core --branch=master --fail-pending || echo 'Not yet passing'"
  },
  "dependencies": {
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.15",
    "@angular/core": "7.2.12",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "^7.2.15",
    "@angular/material": "^7.3.7",
    "@angular/router": "^7.2.15"
  },
  "devDependencies": {
    "@angular-devkit/schematics": "^7.3.9",
    "@angular/animations": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/platform-server": "^7.2.15",
    "@compodoc/gulp-compodoc": "^0.0.10",
    "@types/jasmine": "^3.3.13",
    "@types/jest": "^25.2.0",
    "@types/lodash": "^4.14.134",
    "@types/node": "^10.14.8",
    "angular-cli-ghpages": "^0.5.3",
    "angular2-template-loader": "^0.6.2",
    "ansi-colors": "^3.2.3",
    "autoprefixer": "^9.6.0",
    "awesome-typescript-loader": "^5.2.1",
    "codelyzer": "^4.5.0",
    "commitplease": "^3.2.0",
    "conventional-github-releaser": "2.0.2",
    "copy-webpack-plugin": "^5.0.3",
    "core-js": "^2.6.9",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "del": "^3.0.0",
    "fancy-log": "^1.3.3",
    "greenkeeper-lockfile": "^1.15.1",
    "gulp": "3.9.1",
    "gulp-bump": "^3.1.1",
    "gulp-conventional-changelog": "^2.0.19",
    "gulp-coveralls": "^0.1.4",
    "gulp-file": "^0.4.0",
    "gulp-git": "^2.8.0",
    "gulp-hub": "^0.8.0",
    "gulp-inline-ng2-template": "4.1.0",
    "gulp-tslint": "^8.1.3",
    "html-loader": "^0.5.5",
    "jest": "^23.6.0",
    "jest-cli": "^24.8.0",
    "jest-preset-angular": "^6.0.2",
    "lodash": "^4.17.11",
    "node-sass": "^4.12.0",
    "postcss": "^7.0.17",
    "postcss-strip-inline-comments": "^0.1.5",
    "pump": "^3.0.0",
    "raw-loader": "^1.0.0",
    "rollup": "^1.14.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "3.0.0",
    "run-sequence": "^2.2.1",
    "rxjs": "^6.5.2",
    "sass-loader": "^7.1.0",
    "schematics-utilities": "^1.1.2",
    "source-map-loader": "^0.2.4",
    "to-string-loader": "^1.1.5",
    "travis-status": "^3.0.1",
    "tsickle": "~0.34.3",
    "tslint": "~5.15.0",
    "typescript": "~3.2.4",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-node-externals": "^1.7.2",
    "yargs": "^13.2.4",
    "zone.js": "^0.9.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [
      "<rootDir>/src/"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/config",
      "/dist/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/setupJest.ts"
  },
  "greenkeeper": {
    "ignore": [
      "@angular/core",
      "@angular/common",
      "@angular/compiler",
      "@angular/platform-server",
      "@angular/platform-browser",
      "@angular/platform-browser-dynamic",
      "@angular/compiler-cli",
      "zone.js",
      "rxjs",
      "tslint",
      "gulp-tslint",
      "typescript",
      "awesome-typescript-loader",
      "codelyzer",
      "@angular/animations",
      "@angular/cdk",
      "@angular/material",
      "@angular/flex-layout",
      "@types/jasmine",
      "gulp-inline-ng2-template",
      "conventional-github-releaser",
      "rollup-plugin-uglify"
    ]
  },
  "commitplease": {
    "style": "angular",
    "types": [
      "feat",
      "fix",
      "docs",
      "style",
      "refactor",
      "perf",
      "test",
      "chore",
      "revert",
      "demo"
    ],
    "scope": "\\S+.*"
  }
}
